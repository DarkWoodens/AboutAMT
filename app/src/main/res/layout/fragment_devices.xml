<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="amt_team.center.amtabout.Devices">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="?attr/actionModeSplitBackground"
        android:id="@+id/imageView"
        app:layout_constraintRight_toLeftOf="@+id/cardView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/cardView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.93"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="170dp"
        app:srcCompat="@drawable/cpn3l"
        android:id="@+id/imageView7"
        android:layout_height="300dp"
        android:layout_below="@+id/imageView8"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="170dp"
        app:srcCompat="@drawable/cpn3"
        android:id="@+id/imageView9"
        android:layout_height="300dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="170dp"
        app:srcCompat="@drawable/a7010a48"
        android:id="@+id/imageView8"
        android:layout_height="300dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="170dp"
        app:srcCompat="@drawable/v2viperx"
        android:id="@+id/imageView10"
        android:layout_height="300dp"
        android:layout_below="@+id/imageView9"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
